#include <iostream>
#include <cmath>

using namespace std;

int main()
{
	float a;
	float x;
	float y;
	cout << "enter kg before - ";
	cin >> x;
	cout << "enter price - ";
	cin >> a;
	float r = a / x;
	cout << "1 kilo - " << r << endl;
	cout << "enter kg after - ";
	cin >> y;
	float z = y * r;
	cout << "y kilo - " << z << endl;
}
