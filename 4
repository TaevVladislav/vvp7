#include <iostream>
#include <cmath>

using namespace std;

int main()
{
	float v1;
	float v2;
	float S;
	float t;
	cout << "enter v1 - ";
	cin >> v1;
	cout << "enter v2 - ";
	cin >> v2;
	cout << "enter t - ";
	cin >> t;
	cout << "enter S - ";
	cin >> S;
	float SS = (v1 + v2) * t + S;
	cout << "S at all = " << SS << endl;
}
